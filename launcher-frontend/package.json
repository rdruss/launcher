{
  "name": "launcher-frontend",
  "version": "0.0.7",
  "description": "A frontend for a launcher, microservices booster generator",
  "homepage": "https://developers.redhat.com/launch/",
  "repository": {
    "type": "git",
    "url": "https://github.com/fabric-launcher"
  },
  "contributors": [
    {
      "name": "Erik Jan de Wit",
      "email": "edewit@redhat.com"
    }
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "export TZ=GMT && webpack --config config/webpack.dev.js",
    "build:prod": "webpack --config config/webpack.prod.js -p",
    "check-coverage": "istanbul check-coverage --statements 23 --branches 5 --functions 9 --lines 24",
    "clean-and-copy": "npm run clean-dist && npm run copy-files",
    "clean-dist": "rimraf dist",
    "copy-files": "cpy src/favicon.ico src/assets/settings.json dist",
    "docker:run": "webpack-dev-server --inline --progress --port 8080",
    "functests": "npm-run-all -p webdriver-start testserver-start protractor",
    "lint": "find src -name '*.ts' | xargs tslint -c tslint.json --force",
    "prestart": "npm run clean-and-copy",
    "prebuild": "npm run clean-and-copy",
    "prebuild:prod": "npm run clean-and-copy",
    "protractor": "sleep 15 && ./node_modules/protractor/bin/protractor protractor.config.js",
    "reinstall": "rm -rf node_modules && npm cache clean && npm install",
    "setup": "npm install",
    "start": "webpack-dev-server --inline --progress --host 0.0.0.0 --port 8088",
    "stats": "webpack --profile --json > stats.json",
    "test": "npm-run-all test:unit",
    "test:func": "./run_functional_tests.sh",
    "test:unit": "karma start",
    "testserver-start": "webpack-dev-server --inline --progress --host 0.0.0.0 --port 8088",
    "webdriver-start": "./node_modules/protractor/bin/webdriver-manager update && ./node_modules/protractor/bin/webdriver-manager start",
    "watch:test": "npm run test:unit -- --auto-watch --no-single-run"
  },
  "dependencies": {
    "@angular/common": "4.4.6",
    "@angular/compiler": "4.4.6",
    "@angular/compiler-cli": "4.4.6",
    "@angular/core": "4.4.6",
    "@angular/forms": "4.4.6",
    "@angular/http": "4.4.6",
    "@angular/platform-browser": "4.4.6",
    "@angular/platform-browser-dynamic": "4.4.6",
    "@angular/router": "4.4.6",
    "@angular/upgrade": "4.4.6",
    "bootstrap": "3.3.7",
    "core-js": "2.4.1",
    "jssha": "2.3.1",
    "ngx-forge": "0.0.79",
    "ngx-modal": "0.0.29",
    "patternfly": "3.45.1",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.5.2",
    "uuid": "3.1.0",
    "zone.js": "0.8.4"
  },
  "devDependencies": {
    "@types/core-js": "0.9.34",
    "@types/jasmine": "2.5.35",
    "@types/jssha": "0.0.29",
    "@types/node": "6.0.45",
    "@types/selenium-webdriver": "3.0.8",
    "@types/uuid": "3.4.3",
    "angular2-template-loader": "0.4.0",
    "asciidoctor.js": "1.5.6-preview.5",
    "asciidoctorindex-loader": "0.0.5",
    "autoprefixer": "8.4.1",
    "codelyzer": "4.3.0",
    "cpy-cli": "1.0.1",
    "css-loader": "0.23.1",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "2.0.0-beta.1",
    "file-loader": "0.9.0",
    "ghooks": "2.0.3",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine-core": "2.4.1",
    "json-loader": "0.5.7",
    "json-stringify": "1.0.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage": "1.1.1",
    "karma-htmlfile-reporter": "0.3.4",
    "karma-jasmine": "1.0.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-mocha-reporter": "2.2.5",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "3.0.0",
    "marked": "0.3.19",
    "mocha": "3.0.2",
    "node-sass": "4.7.2",
    "npm-run-all": "2.2.0",
    "null-loader": "0.1.1",
    "opt-cli": "1.4.2",
    "phantomjs-prebuilt": "2.1.7",
    "postcss-loader": "0.9.1",
    "precss": "1.4.0",
    "protractor": "4.0.8",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "1.6.0",
    "rimraf": "2.5.4",
