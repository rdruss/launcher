    "tslint": "5.9.1",
    "tslint-loader": "2.1.5",
    "typescript": "2.3.4",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-middleware": "3.1.3",
    "webpack-dev-server": "3.1.3",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "0.14.0",
    "webpack-validator": "2.1.3"
  }
}
